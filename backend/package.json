{
  "name": "clipiq-backend",
  "version": "1.0.0",
  "description": "Backend API for ClipIQ - YouTube Clone Platform",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "db:migrate": "node src/database/migrate.js",
    "db:seed": "node src/database/seeders/index.js",
    "db:seed-full": "bash src/database/seed-full.sh",
    "db:seed-full-win": "src\\database\\seed-full.bat",
    "db:seed-full-ps1": "powershell -ExecutionPolicy Bypass -File src\\database\\seed-full.ps1",
    "db:verify": "bash src/database/seeds/verify_seed_data.sh",
    "db:verify-win": "src\\database\\seeds\\verify_seed_data.bat",
    "db:verify-ps1": "powershell -ExecutionPolicy Bypass -File src\\database\\seeds\\verify_seed_data.ps1",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "clipiq",
    "video",
    "platform",
    "nodejs",
    "express",
    "postgresql",
    "minio"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "minio": "^7.1.3",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}
